{
    "private": true,
    "scripts": {
        "sdk:clean": "rimraf .sdk.json ./sdk",
        "sdk:json": "node ../../build/bin.js analyze ../server/src .sdk.json",
        "sdk:kit": "node ../../build/bin.js .sdk.json ./sdk -c ../config.ts -n config -r --prettify",
        "sdk:build": "npm-run-all sdk:clean sdk:json sdk:kit"
    },
    "dependencies": {
        "typescript": "^4.3.5"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2"
    }
}
